{
  "name": "vutile",
  "version": "0.0.1",
  "description": "Utilities for clearer, smaller, safer Vue code.",
  "main": "vutile.js",
  "types": "vutile.d.ts",
  "files": [
    "vutile.d.ts",
    "vutile.js",
    "vutile.min.js",
    "vutile.min.js.gz"
  ],
  "scripts": {
    "build": "tsc && npm run minify",
    "test": "jest",
    "lint": "eslint src/ test/",
    "format": "eslint --fix src/ test/",
    "minify": "esbuild --minify --target=es5 index.js > vutile.min.js && gzip -kf9 vutile.min.js && echo \"vutile.min.js.gz :: $( ls -lh vutile.min.js.gz | tr -s ' ' | cut -d' ' -f5 ) :: $( ls -l vutile.min.js.gz | tr -s ' ' | cut -d' ' -f5 )B\"",
    "repl": "node vutile.repl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evnp/vutile.git"
  },
  "keywords": [
    "css",
    "html",
    "typescript",
    "javascript",
    "vue"
  ],
  "author": "Evan Purcer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evnp/vutile/issues"
  },
  "homepage": "https://github.com/evnp/vutile#readme",
  "dependencies": {
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "esbuild": "0.14.11",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
